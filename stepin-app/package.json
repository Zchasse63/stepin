  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "~19.1.0",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
